  <DataSets>
    <DataSet Name="IzvjestajNarudzbi">
      <Query>
        <DataSourceName>Autosalon</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DatumOD">
            <Value>=Parameters!DatumOD.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DatumDo">
            <Value>=Parameters!DatumDo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT P.ImeProizvodjaca,
	   COUNT(NA.AutoID) AS 'Broj automobila',
	   ROUND(SUM(CONVERT(money,NA.NabavnaCijena)),2) AS 'Cijena bez popusta',
	    ROUND(SUM(CONVERT(money,NA.NabavnaCijena-(NA.NabavnaCijena*NA.Popust))),2) AS 'Cijena sa popustom'
FROM Auta AS A
    INNER JOIN Model AS M ON A.ModelID=M.ModelID
    INNER JOIN Proizvodjac AS P ON M.ProizvodjacID=P.ProizvodjacID
    INNER JOIN NabavkeAuta AS NA ON A.AutaID=NA.AutoID
    INNER JOIN Nabavka AS N ON NA.NabavkaID=N.NabavkaID
WHERE N.DatumNarudzbe BETWEEN @DatumOD AND @DatumDo OR @DatumOD IS NULL OR @DatumDo IS NULL
GROUP BY P.ImeProizvodjaca
ORDER BY 2 DESC,1</CommandText>
      </Query>
